#include<stdio.h>
int main()
{
	int arr[10],i,n,max,min;
	printf("enter the number :");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	scanf("%d",&arr[i]);
	max=min=arr[0];
	for(i=1;i<n;i++)
	{
		if(arr[i]>max)
		max=arr[i];
		else if(arr[i]<min)
		min=arr[i];
	}
	printf("maximum %d \n",max);
	printf("minimum %d \n",min);
}
