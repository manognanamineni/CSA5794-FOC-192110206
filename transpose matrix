#include<stdio.h>
int main()
{
	int a[10][10],b[10][10];
	int i,j,r,c,k;
	printf("enter no.of rows:");
	scanf("%d",&r);
	printf("enter no.of columns:");
	scanf("%d",&c);
	printf("enter the elements:");
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			b[i][j]=a[j][i];
		}
	}
	printf("transpose matrix is:");
	for(i=0;i<c;i++)
	{
		for(i=0;i<k;i++)
		{
			printf("%d",b[i][j]);
		}
	}
	return 0;
}
